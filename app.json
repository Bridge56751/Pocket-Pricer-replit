{
  "expo": {
    "name": "Pocket Pricer",
    "slug": "ebay-profit-estimator",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ebayprofit",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ebayprofit.app",
      "buildNumber": "25"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1F2937",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.ebayprofit.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1F2937",
          "dark": {
            "backgroundColor": "#1F2937"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Pocket Pricer to access your camera to scan and identify products."
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
